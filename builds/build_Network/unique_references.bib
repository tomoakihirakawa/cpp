 issn = {10902716},
 journal = {J. Comput. Phys.},
 keywords = {Boundary-integral method,GPU acceleration,Kelvin ship wave patterns,Nonlinear gravity waves,Preconditioned Jacobian-free Newton-Krylov method,Three-dimensional free-surface flows},
 month = {mar},
 pages = {297--313},
 title = {{Jacobian-free Newton-Krylov methods with GPU acceleration for computing nonlinear ship wave patterns}},
 url = {http://arxiv.org/abs/1403.5044 http://dx.doi.org/10.1016/j.jcp.2014.03.024},
