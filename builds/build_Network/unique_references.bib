 journal = {Energies},
 keywords = {DHI shallow water wave Basin,HYDRALAB IV EU FP7 programme,Point absorber,WEC arrays/farms/parks,WECwakes project,Wave basin experiments,Wave energy,Wave energy converters},
 month = {feb},
 number = {2},
 pages = {701--734},
 title = {{Wave Basin Experiments with Large Wave Energy Converter Arrays to Study Interactions between the Converters and Effects on Other Users in the Sea and the Coastal Area}},
 url = {http://www.mdpi.com/1996-1073/7/2/701},
 volume = {7},
