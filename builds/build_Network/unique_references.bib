 title = {{STABILITY OF PERIODIC WAVES OF FINITE AMPLITUDE ON THE SURFACE OF A DEEP FLUID}},
 url = {http://link.springer.com/10.1007/BF00913182 http://link.springer.com/10.1007/BF00913182%5Cnhttp://link.springer.com/article/10.1007/BF00913182},
