@misc{Okamoto2020,
 abstract = {As a calculation method to decide the shape and the installation position of fins installed at the submerged parts of a floating body for its motion reduction, we focus on the calculation method for a viscous fluid by vortex method. As a first step, by restricting 2D problems, we propose a motion calculation method for the floating body based on 2D vortex method using fluid velocity and vorticity as unknown variables. Appling this method to motion calculation of a rectangular floating body, we confirmed its effectiveness through comparison between experimented and calculated results.It is important to promote the use of ocean renewable energy because there is a possibility of not only creating a new energy source but also leading to stable power supply in regions such as remote island areas. As one of ocean renewable energies, wind energy is given. Offshore wind energy generation facilities have less restriction in location and can supply more stable wind energy than on-land facilities. Offshore wind power generation facilities are classified into a bottom mounted type and a floating one. Adoption of the latter is expected in Japan because there are few areas with shallow waters (MLIT, 2014, 2016, 2017).As calculation methods for the viscous fluid proposed previously, mesh method, particle method and vortex method etc. are given. In this research, a fluid domain around the floating body in waves is a high Reynolds number region. In calculation, since vortex generated from the body and its diffusion to the fluid domain should be accurately evaluated, we focus on vortex method.As calculation methods for the viscous fluid proposed previously, mesh method, particle method and vortex method etc. are given. In this research, a fluid domain around the floating body in waves is a high Reynolds number region. In calculation, since vortex generated from the body and its diffusion to the fluid domain should be accurately evaluated, we focus on vortex method.},
 author = {Okamoto, Yutaka and Nagata, Shuichi and Imai, Yasutaka and Niizato, Hideyuki},
 month = {oct},
 title = {{Application of Vortex Method Using Fluid Velocity and Vorticity as Unknown Variables to Motion Analysis of a Floating Body in Waves }},
 year = {2020}
}

