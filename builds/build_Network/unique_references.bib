 isbn = {0123456789},
 issn = {2662-2963},
 journal = {SN Partial Differ. Equations Appl.},
 keywords = {High frequency scattering,Hybrid numerical-asympto,edited by manfred kaltenbacher,high frequency scattering {\'{a}},hybrid numerical-asymptotic boundary element,mathematics subject classification 65n38,method {\'{a}} diffractal {\'{a}},numerical steepest descent {\'{a}},of the topical collection,oscillatory quadrature,papers,this article is part,waves 2019 - invited,{\'{a}} 65r20 {\'{a}} 78a45,{\'{a}} 78m15 {\'{a}} 78m35},
 number = {4},
