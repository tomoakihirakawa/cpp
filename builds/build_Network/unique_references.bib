 doi = {10.1061/(ASCE)WW.1943-5460.0000187},
 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/Atkinson, Smith, Bender - 2012 - Sea Level Rise Effects on Storm Surge and Nearshore Waves on the Texas Coast Influence of Landscape and.pdf:pdf},
 isbn = {0733950X},
 issn = {0733-950X},
 journal = {J. Waterw. Port, Coastal, Ocean Eng.},
 month = {mar},
