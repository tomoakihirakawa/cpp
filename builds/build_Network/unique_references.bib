 isbn = {0029-8018},
 issn = {00298018},
 journal = {Ocean Eng.},
 keywords = {Coastal flooding,Integrated process modeling,Morphological change,Sea level rise,Storms},
 pages = {74--95},
 pmid = {323691100009},
 publisher = {Elsevier},
 title = {{Impact assessment of sea-level rise and hazardous storms on coasts and estuaries using integrated processes model}},
