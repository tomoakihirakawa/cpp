 keywords = {SWAN model,Spectral wave partitioning,Wave energy converters,Wave energy resource assessment},
 pages = {1145--1160},
 title = {{Influence of wave resource assessment methodology on wave energy production estimates}},
 url = {https://ac.els-cdn.com/S0960148115302998/1-s2.0-S0960148115302998-main.pdf?_tid=65921466-9f64-11e7-97d7-00000aacb362&acdnat=1506064101_028a816c92f2efa93678c8f79521c4ec},
 volume = {86},
 year = {2016}
}

