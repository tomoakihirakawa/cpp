 issn = {10902716},
 journal = {J. Comput. Phys.},
 keywords = {Dirichlet boundary conditions,Eulerian SPH,High-order accurate CFD,Neumann boundary conditions,Taylor-Couette flow cellular mutation},
 pages = {109793},
 publisher = {Elsevier Inc.},
 title = {{High-order velocity and pressure wall boundary conditions in Eulerian incompressible SPH}},
 url = {https://doi.org/10.1016/j.jcp.2020.109793},
