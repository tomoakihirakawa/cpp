 issn = {00298018},
 journal = {Ocean Eng.},
 keywords = {Boundary element method,Nonlinear wave-structure interaction,Ocean engineering,Offshore structures},
 number = {September 2018},
 pages = {505--518},
 publisher = {Elsevier Ltd},
 title = {{A 3D parallel boundary element method on unstructured triangular grids for fully nonlinear wave-body interactions}},
 url = {https://doi.org/10.1016/j.oceaneng.2018.09.044},
 volume = {171},
 year = {2019}
