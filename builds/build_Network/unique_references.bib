 url = {https://doi.org/10.1016/j.powtec.2020.09.021},
 volume = {377},
 year = {2021}
}

@article{Liu2022,
 abstract = {The interaction between solid and fluid particles is an important issue in the coupled discrete element method (DEM) – smoothed particle hydrodynamics (SPH) for the liquid-particle analysis of granular materials consisting of irregularly shaped particles. The dilated-polyhedron-based DEM (DPDEM) was employed to simulate irregularly shaped particles. The explicit incompressible SPH (EISPH) was employed to simulate the liquid. A direct coupling model between the SPH particle and the solid wall without fixed particles was derived considering the pressure-induced perpendicular component and viscosity-induced shear component. Meanwhile, the influence of the solid wall on the pressure of SPH particles was considered. Numerical examples were simulated using the proposed DEM–SPH model to investigate the motion of solid particles and variables of the flow field. Previously published data were employed to analyze the validity of the proposed model. It was found that the proposed DEM–SPH model can achieve satisfactory results for the liquid-particle coupling analysis.},
