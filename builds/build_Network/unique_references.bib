@article{TONKIN2003,
 abstract = {A series of scale-model experiments investigated the scouring mechanisms associated with a tsunami impinging on a coastal cylindrical structure. Since scaling effects are significant in sediment transport, a large-scale sediment tank was used. Video images from inside the cylinder elucidated the vortex structures and the time development of scour around the cylinder. The scour development and mechanisms differed according to the sediment substrate - sand or gravel. For gravel, the most rapid scour coincided with the greatest flow velocities. On the other hand, for the sand substrate, the most rapid scour occurred at the end of drawdown - after flow velocities had subsided and shear stresses were presumed to have decreased. This behaviour can be explained in terms of pore pressure gradients. As the water level and velocity subside, the pressure on the sediment bed decreases, creating a vertical pressure gradient within the sand and decreasing the effective stress within the sand. Gravel is too porous to sustain this pressure gradient. During drawdown, the surface pressure decreases approximately linearly from a sustained peak at DeltaP to zero over time DeltaT. The critical fraction Lambda of the buoyant weight of sediment supported by the pore pressure gradient can be estimated as Lambda= 2/rootpi Delta/gammabrootc(nu)DeltaT, in which gamma(b) is the buoyant specific weight of the saturated sediment and c(nu) is the coefficient of consolidation. Much deeper scour was observed where Lambda exceeded one-half.},
 author = {Tonkin, Susan and Yeh, Harry and Kato, Fuminori and Sato, Shinji},
 doi = {10.1017/S0022112003006402},
 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/Tonkin et al. - 2003 - Tsunami scour around a cylinder.pdf:pdf},
 isbn = {0022-1120},
 issn = {00221120},
 journal = {J. Fluid Mech.},
 month = {dec},
 number = {496},
 pages = {165--192},
 title = {{Tsunami scour around a cylinder}},
 url = {http://www.journals.cambridge.org/abstract_S0022112003006402},
 volume = {496},
 year = {2003}
}

@phdthesis{Topper2011,
 abstract = {Kinetic energy extraction devices for ocean and river flows are often located in the\nvicinity of the fluid free surface. This differs from wind turbines where the atmosphere\nmay be considered to extend to infinity for the purposes of numerical modelling. As\nmost kinetic energy extraction devices are based on lifting surfaces, a numerical model\nis sought which can model both lifting and free surface flows. One such model is the\nboundary element method which has been successfully applied to free surface problems\nand to lifting flows as well as the combined problem.\nThis study seeks to develop a high order boundary element method that is capable\nof modelling unsteady lifting and free surface flows in three dimensions. Although high\norder formulations of boundary element methods are common for free surface problems,\nproviding improved accuracy and computational time, their usage for lifting flows is\nless frequent. This may be due to the hypersingular boundary integral equation (HBIE)\nwhich must be solved in order to find the velocity of the vortex wakes behind lifting\nsurfaces.\nIn previous lifting flow studies using high order boundary element methods the\nwake velocities have been determined at the element centres and then interpolated to\nthe collocation points. Not until the paper of Gray et al. (2004b) has a method been\navailable for the direct solution of the HBIEs at the edges of three dimensional high\norder elements with C0 continuous interfaces. The solution employs a technique known\nas the Galerkin boundary element method.\nThis study shows, for the first time, that the Galerkin boundary element method is\napplicable to the solution of the HBIE on the vortex wake of a lifting body. The\napplication of the technique is then demonstrated as part of the numerical model\ndeveloped herein. The model is based on the high order boundary element method\ndeveloped by Xu (1992) for non-linear free surface flows. This formulation is extended\nto include steady uniform flow throughout the computational domain as well as the\npresence of lifting and non-lifting bodies. Several verification cases are implemented to\ntest the accuracy of the model.},
 author = {Topper, Mathew B R},
 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/Topper - 2011 - Numerical modelling of flows involving submerged bodies and free surfaces.pdf:pdf},
 title = {{Numerical modelling of flows involving submerged bodies and free surfaces}},
 url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.860.5884&rep=rep1&type=pdf http://hdl.handle.net/1842/5299},
 year = {2011}
}

@article{Tran-Cong1991,
 abstract = {The boundary-element method is used to solve Stokes equations for periodic arrays of force-free and torque-free rigid particles. Simple cubic arrays of spheres, spheroids, cubes, and clusters of spheres are subjected to a bulk simple shearing flow. The effective volume-averaged stress tensor for the suspension and the detailed velocity and stress fields throughout the Newtonian suspending fluid are calculated. We find that even crude meshes give very good volume-averaged results, but fine meshes are required to track local minima and maxima in the stress field. For simple cubic arrays of spheres, the boundary-element results are in excellent agreement with the analytical viscosity predictions of Nunan & Keller (1984). Even at the highest concentration of solids studied, no significant normal stress differences were observed, in agreement with Nunan & Keller's results (1984). Up to moderate concentrations of particles, the volume-averaged properties of the suspension display only a weak dependence on the particle geometry. Suspensions of spheroids and cubes behave approximately as suspensions of spheres on the average despite large differences in the local micromechanics of stress and velocity fields. Simple cubic arrays of clusters of spheres tend to behave on a macroscopic level as a cubic array of spheres whose effective volume fraction is about 150% of the total volume fraction of the spheres in the clusters. {\textcopyright} 1991, Cambridge University Press. All rights reserved.},
 author = {Phan-Thien, N. and Tran-Cong, T. and Graham, A. L.},
 doi = {10.1017/S0022112091002707},
 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/Phan-Thien, Tran-Cong, Graham - 1991 - Shear flow of periodic arrays of particle clusters A boundary-element method(2).pdf:pdf},
 issn = {14697645},
 journal = {J. Fluid Mech.},
 month = {jul},
 pages = {275--293},
 publisher = {Cambridge University Press},
 title = {{Shear flow of periodic arrays of particle clusters: A boundary-element method}},
 url = {http://www.journals.cambridge.org/abstract_S0022112091002707},
 volume = {228},
 year = {1991}
}

