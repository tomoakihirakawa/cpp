 keywords = {computational fluid mechanics,extreme loads,sloshing,violent waves,wave breaking,wave energy},
 month = {jan},
 number = {1},
 pages = {243--273},
 publisher = {Annual Reviews},
 title = {{Slamming: Recent Progress in the Evaluation of Impact Pressures}},
 url = {http://www.annualreviews.org/doi/10.1146/annurev-fluid-010816-060121},
 volume = {50},
