 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/Kh Ivanov, Kolesov, Vabishchevich - 2019 - Numerical method for recovering the piecewise constant right-hand side function of an ellipti.pdf:pdf},
 issn = {17426596},
 journal = {J. Phys. Conf. Ser.},
 number = {1},
 title = {{Numerical method for recovering the piecewise constant right-hand side function of an elliptic equation from a boundary overdetermination data}},
 volume = {1392},
