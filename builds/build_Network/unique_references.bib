 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/De Koster et al. - 2019 - Extension of B-spline Material Point Method for unstructured triangular grids using Powell-Sabin splines(2).pdf:pdf;:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/De Koster et al. - 2019 - Extension of B-spline Material Point Method for unstructured triangular grids using Powell-Sabin splines(3).pdf:pdf},
 isbn = {4057102000328},
 issn = {21964386},
 journal = {arXiv},
 keywords = {B-splines,Grid-crossing error,Material Point Method,Powell-Sabin splines,Powell–Sabin splines,Unstruc-tured grids,Unstructured grids},
