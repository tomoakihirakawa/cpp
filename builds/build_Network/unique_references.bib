 file = {::},
 issn = {03783839},
 journal = {Coast. Eng.},
 keywords = {3D？,DualSPHysics,Floating bodies,Meshless methods,MoorDyn,Moorings,Numerical modelling,Smoothed particle hydrodynamics,卒研,関連論文},
 mendeley-tags = {3D？,卒研,関連論文},
 number = {September},
 title = {{SPH simulation of floating structures with moorings}},
