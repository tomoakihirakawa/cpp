 issn = {09977546},
 journal = {Eur. J. Mech. B/Fluids},
 keywords = {Helmholtz decomposition,Higher-order boundary element method,Incompressible Navier–Stokes equations},
 month = {mar},
 pages = {94--108},
 title = {{A velocity decomposition approach for three-dimensional unsteady flow}},
 url = {https://linkinghub.elsevier.com/retrieve/pii/S0997754616302448},
 volume = {62},
 year = {2017}
}

