 issn = {10970363},
 journal = {Int. J. Numer. Methods Fluids},
 number = {2},
 pages = {188--222},
 title = {{Nonlinear time-domain wave-structure interaction: A parallel fast integral equation approach}},
 volume = {94},
 year = {2022}
}

