 keywords = {Floating body,Moored body,SPH,Validation data,Wave-structure interaction},
 pages = {306--317},
 publisher = {Elsevier Ltd},
 title = {{Wave interaction with a tethered buoy: SPH simulation and experimental validation}},
 url = {https://doi.org/10.1016/j.oceaneng.2018.03.001},
 volume = {156},
 year = {2018}
}

