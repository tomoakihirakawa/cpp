 journal = {J. Mar. Sci. Appl.},
 keywords = {boundary element method,nonlinear wave,oscillating cylinder,submerged cylinder,wave radiation},
 number = {1},
 pages = {1--10},
 title = {{Modeling of fully nonlinear wave radiation by submerged moving structures using the higher order boundary element method}},
 url = {https://link.springer.com/content/pdf/10.1007%2Fs11804-014-1226-1.pdf},
 volume = {13},
 year = {2014}
}

