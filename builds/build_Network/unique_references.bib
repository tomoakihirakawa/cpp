 keywords = {Arbitrary tangent vectors,Arbitrary topology,Interpolation,Irregular 3D meshes,Modeling,Piecewise polynomial patches,Reconstruction,Surfaces,Triangular patches,Triangulation},
 number = {1},
 pages = {99--109},
 title = {{Polynomial surfaces interpolating arbitrary triangulations}},
 volume = {9},
 year = {2003}
}

@techreport{HAMADAT.1965,
 abstract = {　The Secondary Interactions of Surface Waves are treated in two roughly divided case. In Part Ⅰ, the case in which the first order wave has discrete components is discussed, and in Part Ⅱ th case in which it make the continuous spectrum is computed.
