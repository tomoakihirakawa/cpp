@article{Treyssede2003,
 abstract = {A nonstandard wave equation, established by Galbrun in 1931, is used to study sound propagation in nonuniform flows. Galbrun's equation describes exactly the same physical phenomenon as the linearized Euler's equations (LEE) but is derived from an Eulerian-Lagrangian description and written only in term of the Lagrangian perturbation of the displacement. This equation has interesting properties and may be a good alternative to the LEE: only acoustic displacement is involved (even in nonhomentropic cases), it provides exact expressions of acoustic intensity and energy, and boundary conditions are easily expressed because acoustic displacement whose normal component is continuous appears explicitly. In this paper, Galbrun's equation is solved using a finite element method in the axisymmetric case. With standard finite elements, the direct displacement-based variational formulation gives some corrupted results. Instead, a mixed finite element satisfying the inf-sup condition is proposed to avoid this problem. A first set of results is compared with semianalytical solutions for a straight duct containing a sheared flow (obtained from Pridmore-Brown's equation). A second set of results concerns a more complex duct geometry with a potential flow and is compared to results obtained from a multiple-scale method (which is an adaptation for the incompressible case of Rienstra's recent work).},
 author = {Treyss{\`{e}}de, Fabien and Gabard, Gw{\'{e}}na{\"{e}}l and {Ben Tahar}, Mabrouk},
 doi = {10.1121/1.1534837},
 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/Treyss{\`{e}}de, Gabard, Ben Tahar - 2003 - A mixed finite element method for acoustic wave propagation in moving fluids based on an Euleri.pdf:pdf},
 issn = {0001-4966},
 journal = {J. Acoust. Soc. Am.},
 month = {feb},
 number = {2},
 pages = {705--716},
 pmid = {12597165},
 title = {{A mixed finite element method for acoustic wave propagation in moving fluids based on an Eulerian–Lagrangian description}},
 url = {http://asa.scitation.org/doi/10.1121/1.1534837},
 volume = {113},
 year = {2003}
}

