 title = {{Computations of fully nonlinear three-dimensional wave–wave and wave–body interactions. Part 1. Dynamics of steep three-dimensional waves}},
 url = {https://www.cambridge.org/core/services/aop-cambridge-core/content/view/S0022112001004396 http://journals.cambridge.org/article_S0022112001004396%5Cnhttp://journals.cambridge.org/action/displayAbstract?fromPage=online&aid=79879&fileId=S0022112001004396%5C},
