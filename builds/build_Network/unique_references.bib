 doi = {10.1007/s40571-020-00347-0},
 file = {::},
 isbn = {4057102000},
 issn = {21964386},
 journal = {Comput. Part. Mech.},
 keywords = {Fukushima Dai-ichi Power Plant,Graphics processing unit,Large-scale simulations,Smoothed particle hydrodynamics},
 number = {3},
