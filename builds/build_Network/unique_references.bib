 isbn = {0029-8018},
 issn = {00298018},
 journal = {Ocean Eng.},
 keywords = {Hydrodynamic response,Morphodynamic,Sea level rise,Yangtze Estuary},
 pages = {40--50},
 publisher = {Elsevier},
 title = {{Sea level rise along the East Asia and Chinese coasts and its role on the morphodynamic response of the Yangtze River Estuary}},
 url = {http://dx.doi.org/10.1016/j.oceaneng.2013.03.014},
 volume = {71},
