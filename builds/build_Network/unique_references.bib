}

@article{Ma2006,
 abstract = {This paper presents a newly developed quasi arbitrary Lagrangian-Eulerian finite element method (QALE-FEM) for simulating water waves based on fully nonlinear potential theory. The main difference of this method from the conventional finite element method developed by one of authors of this paper and others (see e.g. [Q.W. Ma, G.X. Wu, R. Eatock Taylor, Finite element simulation of fully non-linear interaction between vertical cylinders and steep waves. Part 1: Methodology and numerical procedure and Part 2: Numerical results and validation, Int. J. Numer. Methods Fluids, 36 (2001) 265-308.] and [G.X. Wu, Z.Z. Hu, Simulation of nonlinear interactions between waves and floating bodies through a finite-element-based numerical tank, Proc. R. Soc. A 460 (2004) 2050, 3037-3058.]) is that the complex mesh is generated only once at the beginning and is moved at all other time steps in order to conform to the motion of the free surface and structures. This feature allows one to use an unstructured mesh with any degree of complexity without the need of regenerating it every time step, which is generally inevitable and very costly. Due to this feature, the QALE-FEM has high potential in enhancing computational efficiency when applied to problems associated with the complex interaction between large steep waves and structures since the use of an unstructured mesh in such a case is likely to be necessary. To achieve overall high efficiency, the numerical techniques involved in the QALE-FEM are developed, including the method to move interior nodes, technique to re-distribute the nodes on the free surface, scheme to calculate velocities and so on. The model is validated by water waves generated by a wavemaker in a tank and the interaction between water waves and periodic bars on the bed of tank. Satisfactory agreement is achieved with analytical solutions, experimental data and numerical results from other methods. {\textcopyright} 2005 Elsevier Inc. All rights reserved.},
 author = {Ma, Q. W. and Yan, S.},
 doi = {10.1016/j.jcp.2005.06.014},
 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/Ma, Yan - 2006 - Quasi ALE finite element method for nonlinear water waves.pdf:pdf},
 issn = {00219991},
 journal = {J. Comput. Phys.},
 keywords = {Free surface,Nonlinear water waves,Periodic bars,QALE-FEM,Spring analogy},
 month = {feb},
 number = {1},
 pages = {52--72},
 publisher = {Academic Press},
 title = {{Quasi ALE finite element method for nonlinear water waves}},
 url = {https://www.sciencedirect.com/science/article/pii/S0021999105003098},
 volume = {212},
 year = {2006}
}

@article{Ma2009,
 abstract = {This paper presents further development of the quasi arbitrary Lagrangian–Eulerian finite element method (QALE-FEM) based on a fully non-linear potential theory to numerically simulate non-linear responses of 3D moored floating bodies to steep waves. In the QALE-FEM (recently developed by the authors and applied to 2D floating bodies), the complex unstructured mesh is generated only once at the beginning of calculation and is moved to conform to the motion of boundaries at other time steps by using a robust spring analogy method specially suggested for these kind of problems, avoiding the necessity of high-cost remeshing. In order to tackle challenges associated with 3D floating bodies, several new numerical techniques are developed in this paper. These include the technique for moving the mesh near body surfaces, the scheme for calculating velocity on 3D body surfaces and the modified semi-implicit time integration method for floating bodies procedure (ISITIMFB-M) that is more efficient for dealing with the full coupling between waves and bodies. Using the newly developed techniques and methods, various cases for 3D floating bodies with motions of up to six degrees of freedom (DoFs) are simulated. These include a SPAR platform, a barge-type floating body and one or two Wigley Hulls in head seas or in oblique waves. For some selected cases, the numerical results are compared with experimental data available in the public domain and satisfactory agreements are achieved. Many results presented in this paper have not been found elsewhere to the best knowledge of the authors},
 archiveprefix = {arXiv},
 arxivid = {1010.1724},
 author = {Ma, Q. W. and Yan, S.},
 doi = {10.1002/nme.2505},
 eprint = {1010.1724},
 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/Ma, Yan - 2009 - QALE-FEM for numerical modelling of non-linear interaction between 3D moored floating bodies and steep waves.pdf:pdf},
 isbn = {978-1-4577-0079-8},
 issn = {00295981},
 journal = {Int. J. Numer. Methods Eng.},
 keywords = {3D floating bodies,Freely responses to waves with six degrees of free,Iterative procedure,Non-linear water waves,QALE-FEM,Spring analogy method},
 month = {may},
 number = {6},
 pages = {713--756},
 pmid = {260949200001},
 title = {{QALE-FEM for numerical modelling of non-linear interaction between 3D moored floating bodies and steep waves}},
 url = {http://doi.wiley.com/10.1002/nme.2505},
 volume = {78},
 year = {2009}
}

