 month = {apr},
 number = {1703},
 pages = {471--488},
 title = {{The instabilities of gravity waves of finite amplitude in deep water I. Superharmonics}},
 url = {https://royalsocietypublishing.org/doi/10.1098/rspa.1978.0080},
 volume = {360},
 year = {1978}
}

