@article{Ewing1983,
 abstract = {Surface gravity waves generated by the wind have been studied extensively over the last twenty-five years. Advances in the theories of wave generation together with field measurements have now clarified the principal means by which waves grow. Weak non-linear wave-wave interactions have been found to have an important role in controlling the evolution of the wind-wave spectrum, as shown in the Joint North Sea Wave Project. However, a major area of future research is in the study of breaking waves. This highly non-linear phenom-enon still defies detailed analysis and is very difficult to observe quanti-tatively. Wave prediction methods based on the energy balance equation are discussed together with a short review on their application to engineering design stupies.},
 author = {Ewing, J A},
 doi = {10.1111/j.1365-246X.1983.tb01882.x},
