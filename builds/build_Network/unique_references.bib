 doi = {10.1017/S0022112061000913},
 file = {:Users/tomoaki/Library/Application Support/Mendeley Desktop/Downloaded/Phillips - 1961 - On the dynamics of unsteady gravity waves of finite amplitude Part 2. Local properties of a random wave field.pdf:pdf},
 isbn = {0022-1120},
