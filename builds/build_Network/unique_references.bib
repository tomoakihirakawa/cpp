 issn = {09557997},
 journal = {Eng. Anal. Bound. Elem.},
 keywords = {Arbitrary Lagrangian Eulerian formulation,Boundary element method,Nonlinear free-surface problems,Semi-Lagrangian formulation,Unsteady ship-wave interaction},
 number = {1},
 pages = {128--143},
 publisher = {Elsevier},
 title = {{A stable and adaptive semi-Lagrangian potential model for unsteady and nonlinear ship-wave interactions}},
