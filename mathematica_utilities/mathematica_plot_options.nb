Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "interpolateAndFourier", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"interpolateAndFourier", "[", 
    RowBox[{"time_List", ",", "disp_List"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "interpdata", ",", "tmp", ",", "interpFunc", ",", "interpData", ",", 
       "fft", ",", "Fs", ",", "freq", ",", "n", ",", "tstart", ",", "tend", 
       ",", "c"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"n", "=", 
       RowBox[{"Length", "[", "time", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"tstart", "=", 
       RowBox[{"time", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"tend", "=", 
       RowBox[{"time", "[", 
        RowBox[{"[", 
         RowBox[{"-", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"c", "=", 
       RowBox[{"2", "/", 
        RowBox[{"Sqrt", "[", "n", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"tmp", "=", 
       RowBox[{"Interpolation", "[", 
        RowBox[{"Transpose", "[", 
         RowBox[{"{", 
          RowBox[{"time", ",", "disp"}], "}"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"interpdata", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"t", ",", 
           RowBox[{"tmp", "[", "t", "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", 
           RowBox[{"Subdivide", "[", 
            RowBox[{"tstart", ",", "tend", ",", 
             RowBox[{"n", "-", "1"}]}], "]"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"fft", "=", 
       RowBox[{"c", "*", 
        RowBox[{"Abs", "[", 
         RowBox[{"Fourier", "[", 
          RowBox[{"interpdata", "[", 
           RowBox[{"[", 
            RowBox[{";;", ",", "2"}], "]"}], "]"}], "]"}], "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Fs", "=", 
       RowBox[{"n", "/", 
        RowBox[{"(", 
         RowBox[{"tend", "-", "tstart"}], ")"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"freq", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"i", "*", 
          RowBox[{"Fs", "/", "n"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "0", ",", 
           RowBox[{
            RowBox[{"n", "/", "2"}], "-", "1"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"Transpose", "@", 
        RowBox[{"{", 
         RowBox[{"freq", ",", 
          RowBox[{"fft", "[", 
           RowBox[{"[", 
            RowBox[{";;", 
             RowBox[{"Length", "[", "freq", "]"}]}], "]"}], "]"}]}], "}"}]}], 
